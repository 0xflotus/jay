{
	"name": "jay-prerelease",
	"version": "0.0.0",
	"description": "Supercharged node.js repl",
	"license": "MIT",
	"repository": "nikersify/jay",
	"author": {
		"name": "Maciej ≈Åaszcz",
		"email": "nikersify@nikerino.com",
		"url": "nikerino.com"
	},
	"bin": {
		"jay": "dist/cli.js"
	},
	"scripts": {
		"build": "del dist && tsc",
		"build:watch": "tsc -w",
		"prepublishOnly": "npm run build",
		"test": "xo && ava -- --with-npm"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"acorn": "^6.1.1",
		"acorn-walk": "^6.1.1",
		"ansi-escapes": "^4.1.0",
		"ast-types": "^0.12.4",
		"builtin-modules": "^3.1.0",
		"chalk": "^2.4.2",
		"env-paths": "^2.2.0",
		"execa": "^1.0.0",
		"figures": "^3.0.0",
		"got": "^9.6.0",
		"import-fresh": "^3.0.0",
		"io-ts": "^1.8.6",
		"lodash": "^4.17.11",
		"make-dir": "^3.0.0",
		"ora": "^3.4.0",
		"pacote": "^9.5.0",
		"read-pkg-up": "^6.0.0",
		"resolve": "^1.10.1",
		"s-ago": "^2.0.1",
		"strip-ansi": "^5.2.0",
		"wrap-ansi": "^5.1.0",
		"wrap-await": "^1.0.0"
	},
	"devDependencies": {
		"@nikersify/noop": "^1.0.0",
		"@types/execa": "^0.9.0",
		"@types/lodash": "^4.14.123",
		"@types/node": "^11.13.5",
		"@types/resolve": "0.0.8",
		"@types/wrap-ansi": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^1.6.0",
		"ava": "^1.4.1",
		"del-cli": "^1.1.0",
		"eslint-config-xo-typescript": "^0.9.0",
		"p-series": "^2.1.0",
		"tempy": "^0.3.0",
		"ts-node": "^8.1.0",
		"typescript": "^3.4.3",
		"xo": "^0.24.0"
	},
	"ava": {
		"babel": false,
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"xo": {
		"semicolon": false,
		"extends": "xo-typescript",
		"extensions": [
			"ts",
			"tsx"
		],
		"rules": {
			"@typescript-eslint/explicit-function-return-type": false,
			"@typescript-eslint/member-delimiter-style": [
				"error",
				{
					"multiline": {
						"delimiter": "none"
					},
					"singleline": {
						"delimiter": "semi",
						"requireLast": false
					}
				}
			],
			"ava/no-ignored-test-files": false,
			"//": "current version doesn't work with typescript type exports, waiting for fix to be released",
			"import/named": false,
			"quotes": [
				"error",
				"single",
				{
					"avoidEscape": true
				}
			]
		}
	}
}
